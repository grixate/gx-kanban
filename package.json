{
  "name": "gx-kanban",
  "version": "1.0.1",
  "description": "Obsidian-native Kanban board plugin",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "keywords": [
    "obsidian",
    "kanban"
  ],
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "9.30.1",
    "@types/node": "^20.14.11",
    "esbuild": "0.25.5",
    "eslint-plugin-obsidianmd": "0.1.9",
    "globals": "14.0.0",
    "jiti": "2.6.1",
    "tslib": "2.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.35.1",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "obsidian": "latest",
    "yaml": "^2.6.1"
  }
}
